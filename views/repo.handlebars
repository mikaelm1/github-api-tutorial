{{> header}}
<div class="col-md-4 col-md-offset-4">
    <h1>Repos for User: {{userName}}</h1>
    {{#each repoArray}}
        <div class="list-group">
        <a href="#" class="list-group-item">
            <h4 class="list-group-item-heading">Name: {{this.repoName}}</h4>
            <p class="list-group-item-text"><b>Created At: </b>{{this.repoCreated}}</p>
            <p class="list-group-item-text"><b>Description: </b>{{this.repoDesc}}</p>
        </a>
        </div>
    {{/each}}
</div>

